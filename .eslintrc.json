{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals":{"Horizon": true, "Vue": true},
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "warn",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "prefer-arrow-callback": "warn"
    }
}
